project (dynamic_simulation) : using_madara, using_ace, using_gams, using_vrep {
  exeout = $(GAMS_ROOT)/bin
  exename = dynamic_simulation

  Documentation_Files {
    INSTALL.txt
    LICENSE.txt
    README.txt
    VERSION.txt
  }
  
  Build_Files {
    using_ace.mpb
    using_madara.mpb
    using_vrep.mpb
    using_gams.mpb
  }

  Source_Files {

    Simulation {
      src/simulation
    }

    Utility {
      src/gams/utility
    }
  }
}
